# Production environment configuration template
# Copy this to .env.production and fill in your actual values

# 중요: NEXT_PUBLIC_IS_DEV_MODE는 프로덕션에서 false이거나 설정하지 않아야 함
# NEXT_PUBLIC_IS_DEV_MODE=false

# Supabase 설정 - 실제 값으로 교체하세요
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url-here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# 프로덕션 URL 설정
NEXTAUTH_URL=https://your-production-url.com
NEXTAUTH_SECRET=your-production-secret-here

# 스크립트용 서비스 롤 키 (로컬에서만 사용, 절대 커밋하지 않기)
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here